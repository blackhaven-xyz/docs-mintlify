    "name": "Launch App",
    "url": "https://blackhaven.io"
  },
  "anchors": [
    {
      "name": "Twitter",
      "icon": "x-twitter",
      "url": "https://x.com/blackhaven"
    },
    {
      "name": "Discord",
      "icon": "discord",
      "url": "https://discord.gg/GfTkD6xaGC"
    }
  ],
  "navigation": [
    {
      "group": "Getting Started",
      "pages": ["overview"]
    },
    {
      "group": "Reserve Backed Token",
      "pages": [
        "rbt/overview",
        {
          "group": "RBT Notes",
          "pages": ["rbt/notes", "rbt/enhanced-bond-discounts"]
        }
      ]
    },
    {
      "group": "Products",
      "pages": [
        "products/fixed-term-bonds",
        {
          "group": "Blackhaven DEX",
          "pages": [
            "products/blackhaven-dex",
            "products/dex/swap",
            "products/dex/liquidity-provision",
            "products/dex/liquidity-bonds",
            "products/dex/dca"
          ]
        },

        "products/haven-protected-notes",
        "treasury/bam"
      ]
    },
    {
      "group": "HVN Token",
      "pages": [
        "hvn/governance",
        "hvn/staking",
        "hvn/proximity",
        "hvn/bribe-market"
      ]
    },
    {
      "group": "Treasury",
      "pages": ["treasury/overview", "treasury/fees"]
    },
    {
      "group": "Resources",
      "pages": ["resources/risks", "resources/glossary"]
    }
  ],
  "footerSocials": {
    "x": "https://x.com/blackhaven",
    "discord": "https://discord.gg/GfTkD6xaGC"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "seo": {
    "indexHiddenPages": false,
    "metatags": {
      "og:site_name": "Blackhaven Documentation",
      "og:description": "The reserve-backed liquidity engine powering sustainable rewards and treasury growth across the MegaETH ecosystem.",
      "og:image": "https://docs.blackhaven.xyz/images/banner.webp",
      "twitter:site": "@blackhaven"
    }
  }
}
