{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Blackhaven",
  "logo": {
    "dark": "/logo/DARKMODE.png",
    "light": "/logo/LIGHTMODE.png"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#D4FFAF",
    "light": "#D4FFAF",
    "dark": "#D4FFAF",
    "background": {
      "dark": "#0A0C14",
      "light": "#FFFFFF"
    },
    "anchors": {
      "from": "#D4FFAF",
      "to": "#D4FFAF"
    }
  },
  "font": {
    "headings": {
      "family": "Inter",
      "weight": 700
    },
    "body": {
      "family": "Inter",
      "weight": 400
    }
  },
  "topbarLinks": [],
